#!/bin/bash

echo "ctrl-e - end of string"
echo "ctrl-a - beginning of string"
echo "ctrl-u - delete string before cursor"
echo "ctrl-y - undo"
echo "ctrl-k - delete string after cursror"
echo "ctrl-w - delete word before cursor"
echo "ctrl-b - move cursor one word after"
echo "ctrl-w - move cursor one word before"
