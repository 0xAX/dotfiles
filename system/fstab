/dev/mapper/fedora-root /                       ext4    defaults,noatime,discard        0 2
UUID=2983373c-5f00-40a3-b151-eef19b43bca9 /boot         ext4    defaults,noatime,discard        0 2
/dev/mapper/fedora-home /home                   ext4    defaults,noatime,discard        0 2
/dev/mapper/fedora-swap swap                    swap    defaults        0 0

/dev/sda1 /home/alex/olddisk ext4 defaults,noatime 0 1
/dev/sdd1 /home/alex/disk    ext4 defaults,noatime,nodiratime 0 2
/dev/sdc1 /home/alex/disk2   ext4 defaults,noatime,nodiratime 0 2

tmpfs /tmp                         tmpfs defaults 0 0
tmpfs /var/tmp                     tmpfs defaults 0 0
tmpfs /home/alex/.cache            tmpfs defaults,noatime,mode=1777 0 0
