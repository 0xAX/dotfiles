# ip
alias ip="dig +short myip.opendns.com @resolver1.opendns.com"
alias localip="ipconfig getifaddr en0"

# nc
alias tcpListen="nc -l -t "
alias udpListen="nc -u 53"
alias scantcpPort="nc -zv "

# netstat
alias socketsState="netstat -a"
alias autoConnectionsState="netstat -i"
alias whoConnect="netstat -p"